{
  "name": "Custom Web Shortcuts",
  "version": "1.0.0",
  "default_locale": "en",
  "description": "__MSG_description__",
  "manifest_version": 3,
  "icons": {
    "128": "icons/arrows.png"
  },
  "action": {
    "default_icon": {
      "128": "icons/arrows.png"
    },
    "default_title": "Custom Web Shortcuts",
    "default_popup": "popup/popup.html"
  },
  "background": {
    "service_worker": "background/background.js"
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "all_frames": true,
      "js": ["content/content.js"],
      "run_at": "document_start"
    }
  ],
  "commands": {
    "next-chapter": {
      "suggested_key": {
        "default": "Ctrl+Right",
        "mac": "Command+Right"
      },
      "description": "Go to next chapter."
    },
    "prev-chapter": {
      "suggested_key": {
        "default": "Ctrl+Left",
        "mac": "Command+Left"
      },
      "description": "Go to previous chapter."
    },
    "player-load": {
      "suggested_key": {
        "default": "Ctrl+Space",
        "mac": "Command+Space"
      },
      "description": "Load the player."
    },
    "hotkey-4": {
      "suggested_key": {
        "default": "Ctrl+Space",
        "mac": "Command+Space"
      },
      "description": "Hotkey 4."
    },
    "hotkey-5": {
      "description": "Hotkey 5."
    },
    "hotkey-6": {
      "description": "Hotkey 6."
    },
    "hotkey-7": {
      "description": "Hotkey 7."
    },
    "hotkey-8": {
      "description": "Hotkey 8."
    },
    "hotkey-9": {
      "description": "Hotkey 9."
    },
    "hotkey-10": {
      "description": "Hotkey 10."
    },
    "hotkey-11": {
      "description": "Hotkey 11."
    }
  },
  "permissions": ["activeTab", "scripting", "storage"],
  "host_permissions": ["<all_urls>"]
}