{
    "name": "go through the chapters",
    "version": "0.0.0.1",
    "manifest_version": 3,
    "description": "An extension to go through the chapters in some webpages.",
    "icons": {
        "128": "icons/arrows.png"
    },
    "background": {
        "service_worker": "js/service-worker.js"
    },
    "permissions": ["<all_urls>", "tabs" ,"activeTab", "scripting"],
    "host_permissions": ["<all_urls>"],
    "commands": {
        "next-chapter": {
            "suggested_key": {
                "default": "Ctrl+Right",
                "mac": "Command+Right"
            },
            "description": "Go to next chapter."
        },
        "prev-chapter":  {
            "suggested_key": {
                "default": "Ctrl+Left",
                "mac": "Command+Left"
            },
            "description": "Go to previous chapter."
        }
    }
   
}